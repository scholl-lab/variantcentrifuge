{
  "reference": "GRCh37.p13",
  "add_chr": false,
  "filters": "",
  "fields_to_extract": "CHROM POS REF ALT ID FILTER QUAL AC ANN[0].GENE ANN[0].FEATUREID ANN[0].EFFECT ANN[0].IMPACT ANN[0].HGVS_C ANN[0].HGVS_P NMD[0].PERC ANN[0].AA_POS ANN[0].AA_LEN dbNSFP_REVEL_score dbNSFP_CADD_phred splice_dbscSNV_rf_score splice_dbscSNV_ada_score splice_spidex_dpsi_zscore dbNSFP_ALFA_Total_AC dbNSFP_clinvar_clnsig ClinVar_CLNSIG hgmd_CLASS GEN[*].GT",
  "sample_file": "samples.txt",
  "replace_script_options": "--append-genotype",
  "output_file": "variants.tsv",
  "interval_expand": 1000,
  "use_phenotype_filtering": false,
  "perform_gene_burden": false,
  "keep_intermediates": false,

  "igv_enabled": false,
  "igv_reference": null,
  "igv_fasta": null,
  "igv_ideogram": null,
  "igv_flanking": 50,

  "igv_max_allele_len_filename": 10,
  "igv_hash_len_filename": 6,
  "igv_max_variant_part_filename": 50,

  "default_field_profile": "dbnsfp4",
  "field_profiles": {
    "dbnsfp4": {
      "description": "dbNSFP v4.x (gnomAD v3/v4.0, separate exome+genome)",
      "fields_to_extract": "dbNSFP_gnomAD_exomes_AC dbNSFP_gnomAD_genomes_AC dbNSFP_gnomAD_exomes_AF dbNSFP_gnomAD_genomes_AF dbNSFP_hg19_chr dbNSFP_hg19_pos_1_based_",
      "hidden_columns": ["dbNSFP_gnomAD_exomes_AC", "dbNSFP_gnomAD_genomes_AC"],
      "fragments": {
        "gnomad_af_below": "((dbNSFP_gnomAD_exomes_AF[0] < {0}) | (na dbNSFP_gnomAD_exomes_AC[0])) & ((dbNSFP_gnomAD_genomes_AF[0] < {0}) | (na dbNSFP_gnomAD_genomes_AC[0]))",
        "gnomad_af_below_or": "(dbNSFP_gnomAD_exomes_AF[0] < {0}) | (na dbNSFP_gnomAD_exomes_AC[0]) | (dbNSFP_gnomAD_genomes_AF[0] < {0}) | (na dbNSFP_gnomAD_genomes_AC[0])",
        "gnomad_ac_atmost": "((dbNSFP_gnomAD_exomes_AC[0] <= {0}) | (na dbNSFP_gnomAD_exomes_AC[0])) & ((dbNSFP_gnomAD_genomes_AC[0] <= {0}) | (na dbNSFP_gnomAD_genomes_AC[0]))"
      }
    },
    "dbnsfp5": {
      "description": "dbNSFP v5.x (gnomAD v4.1, joint exome+genome)",
      "fields_to_extract": "dbNSFP_gnomAD4.1_joint_AC dbNSFP_gnomAD4.1_joint_AF dbNSFP_hg19_chr dbNSFP_hg19_pos_1_based_",
      "hidden_columns": ["dbNSFP_gnomAD4.1_joint_AC"],
      "fragments": {
        "gnomad_af_below": "((dbNSFP_gnomAD4.1_joint_AF[0] < {0}) | (na dbNSFP_gnomAD4.1_joint_AC[0]))",
        "gnomad_af_below_or": "(dbNSFP_gnomAD4.1_joint_AF[0] < {0}) | (na dbNSFP_gnomAD4.1_joint_AC[0])",
        "gnomad_ac_atmost": "((dbNSFP_gnomAD4.1_joint_AC[0] <= {0}) | (na dbNSFP_gnomAD4.1_joint_AC[0]))"
      }
    }
  },

  "presets": {
    "pass": "(FILTER = 'PASS')",
    "not_artefact": "((AC[ANY] <= 513) & (QUAL[ANY] >= 100))",
    "not_benign": "(!(dbNSFP_clinvar_clnsig =~ '[Bb]enign') & !(ClinVar_CLNSIG =~ '[Bb]enign'))",
    "5percent_or_pathogenic": "(({{gnomad_af_below_or:0.05}}) | (((dbNSFP_clinvar_clnsig =~ '[Pp]athogenic') & !(dbNSFP_clinvar_clnsig =~ '[Cc]onflicting')) | ((ClinVar_CLNSIG =~ '[Pp]athogenic') & !(ClinVar_CLNSIG =~ '[Cc]onflicting'))))",
    "5percent": "({{gnomad_af_below:0.05}})",
    "1percent": "({{gnomad_af_below:0.001}})",
    "rare": "({{gnomad_af_below:0.0001}})",
    "super_rare": "({{gnomad_ac_atmost:2}})",
    "pathogenic": "(((dbNSFP_clinvar_clnsig =~ '[Pp]athogenic') & !(dbNSFP_clinvar_clnsig =~ '[Cc]onflicting')) | ((ClinVar_CLNSIG =~ '[Pp]athogenic') & !(ClinVar_CLNSIG =~ '[Cc]onflicting')))",
    "coding": "((ANN[ANY].IMPACT has 'HIGH') | (ANN[ANY].IMPACT has 'MODERATE'))",
    "high": "((ANN[ANY].IMPACT has 'HIGH'))",
    "moderate": "((ANN[ANY].IMPACT has 'MODERATE'))",
    "moderate_and_high_prediction": "(((ANN[ANY].IMPACT has 'MODERATE')) & ((dbNSFP_REVEL_score >= 0.9 | (dbNSFP_CADD_phred >= 30)))",
    "high_splice_prediction": "(((ANN[ANY].IMPACT has 'MODERATE')) & ((splice_spidex_dpsi_zscore <= -2.0 ) & ((splice_dbscSNV_ada_score >= 0.6 ) | (splice_dbscSNV_rf_score >= 0.6 ))))",
    "low": "((ANN[ANY].IMPACT has 'LOW'))",
    "high_or_moderate": "((ANN[ANY].IMPACT has 'HIGH') | (ANN[ANY].IMPACT has 'MODERATE'))",
    "high_or_moderate_or_low": "((ANN[ANY].IMPACT has 'HIGH') | (ANN[ANY].IMPACT has 'MODERATE') | (ANN[ANY].IMPACT has 'LOW'))",
    "high_or_moderate_or_high_splice_prediction": "(((ANN[ANY].IMPACT has 'HIGH') | (ANN[ANY].IMPACT has 'MODERATE')) | ((splice_spidex_dpsi_zscore <= -2.0 ) & ((splice_dbscSNV_ada_score >= 0.6 ) | (splice_dbscSNV_rf_score >= 0.6 ))))",
    "high_or_moderate_or_high_splice_prediction_or_high_prediction": "(((ANN[ANY].IMPACT has 'HIGH') | (ANN[ANY].IMPACT has 'MODERATE')) | ((splice_spidex_dpsi_zscore <= -2.0 ) & ((splice_dbscSNV_ada_score >= 0.6 ) | (splice_dbscSNV_rf_score >= 0.6 ))) | ((dbNSFP_REVEL_score >= 0.9) | (dbNSFP_CADD_phred >= 30)))",
    "modifier": "((ANN[ANY].IMPACT has 'MODIFIER'))",
    "high_or_lof_or_nmd": "(((exists LOF[*].PERC) & (LOF[*].PERC > 0.9)) | ((exists NMD[*].PERC) & (NMD[*].PERC > 0.9)) | (ANN[ANY].IMPACT has 'HIGH')))",
    "high_or_pathogenic": "(((ANN[ANY].IMPACT has 'HIGH') | (((dbNSFP_clinvar_clnsig =~ '[Pp]athogenic') & !(dbNSFP_clinvar_clnsig =~ '[Cc]onflicting')) | ((ClinVar_CLNSIG =~ '[Pp]athogenic') & !(ClinVar_CLNSIG =~ '[Cc]onflicting'))))",
    "aif": "((({{gnomad_af_below_or:0.05}}) | (((dbNSFP_clinvar_clnsig =~ '[Pp]athogenic') & !(dbNSFP_clinvar_clnsig =~ '[Cc]onflicting')) | ((ClinVar_CLNSIG =~ '[Pp]athogenic') & !(ClinVar_CLNSIG =~ '[Cc]onflicting')))) & ((((dbNSFP_clinvar_clnsig =~ '[Pp]athogenic') & !(dbNSFP_clinvar_clnsig =~ '[Cc]onflicting')) | ((ClinVar_CLNSIG =~ '[Pp]athogenic') & !(ClinVar_CLNSIG =~ '[Cc]onflicting'))) | (({{gnomad_af_below_or:0.01}}) & ((splice_spidex_dpsi_zscore <= -2.0 ) & (( splice_dbscSNV_ada_score >= 0.6 ) | ( splice_dbscSNV_rf_score >= 0.6 )))) | (({{gnomad_af_below_or:0.01}}) & ((ANN[ANY].IMPACT has 'MODERATE') & ((dbNSFP_REVEL_score >= 0.9) | (dbNSFP_CADD_phred >= 30)))) | (({{gnomad_af_below_or:0.01}}) & (((exists LOF[*].PERC) & (LOF[*].PERC > 0.9)) | ((exists NMD[*].PERC) & (NMD[*].PERC > 0.9)) | (ANN[ANY].IMPACT has 'HIGH')))))",
    "mutect2_TvsN_pass": "((({{gnomad_ac_atmost:2}}) | ((exists ID) & ( ID =~ 'COS' ))) & (( na FILTER ) | (FILTER = 'PASS')) & ( GEN[0].DP >= 50 ) & ((GEN[1].AF < 0.03) | (na GEN[1].AF)) & ( GEN[0].AF >= 0.05 ))",
    "mutect2_TvsN": "((({{gnomad_ac_atmost:2}}) | ((exists ID) & ( ID =~ 'COS' ))) & ( GEN[*].DP >= 50 ) & ((GEN[1].AF < 0.03) | (na GEN[1].AF)) & ( GEN[0].AF >= 0.05 ))",
    "mutect2_To_pass": "((({{gnomad_ac_atmost:2}}) | ((exists ID) & ( ID =~ 'COS' ))) & (( na FILTER ) | (FILTER = 'PASS')) & ( GEN[*].DP >= 50 ) & ( GEN[0].AF >= 0.05 ))",

    "somatic": "({{gnomad_ac_atmost:2}}) & (GEN[{normal_idx}].DP >= {normal_dp_min}) & (GEN[{tumor_idx}].DP >= {tumor_dp_min}) & ((GEN[{normal_idx}].AF < {normal_af_max}) | (na GEN[{normal_idx}].AF)) & (GEN[{tumor_idx}].AF >= {tumor_af_min})",
    "somatic_pass": "({{gnomad_ac_atmost:2}}) & ((na FILTER) | (FILTER = 'PASS')) & (GEN[{normal_idx}].DP >= {normal_dp_min}) & (GEN[{tumor_idx}].DP >= {tumor_dp_min}) & ((GEN[{normal_idx}].AF < {normal_af_max}) | (na GEN[{normal_idx}].AF)) & (GEN[{tumor_idx}].AF >= {tumor_af_min})",
    "somatic_strict": "({{gnomad_ac_atmost:2}}) & ((na FILTER) | (FILTER = 'PASS')) & (GEN[{normal_idx}].DP >= 50) & (GEN[{tumor_idx}].DP >= 50) & ((GEN[{normal_idx}].AF < 0.01) | (na GEN[{normal_idx}].AF)) & (GEN[{tumor_idx}].AF >= 0.05)",
    "loh": "(GEN[{normal_idx}].GT = '0/1') & ((GEN[{tumor_idx}].GT = '1/1') | (GEN[{tumor_idx}].AF >= 0.85)) & (GEN[{tumor_idx}].DP >= {tumor_dp_min}) & (GEN[{normal_idx}].DP >= {normal_dp_min})",
    "germline_shared": "((GEN[{normal_idx}].AF >= 0.20) & (GEN[{tumor_idx}].AF >= 0.20)) & (GEN[{normal_idx}].DP >= {normal_dp_min}) & (GEN[{tumor_idx}].DP >= {tumor_dp_min})",
    "tumor_only": "({{gnomad_ac_atmost:2}}) & ((na FILTER) | (FILTER = 'PASS')) & (GEN[{tumor_idx}].DP >= {tumor_dp_min}) & (GEN[{tumor_idx}].AF >= {tumor_af_min})"
  },
  "links": {
    "SpliceAI": "https://spliceailookup.broadinstitute.org/#variant={CHROM}-{POS}-{REF}-{ALT}&hg=19&bc=basic&distance=500&mask=0&ra=0",
    "Franklin": "https://franklin.genoox.com/clinical-db/variant/snp/{CHROM}-{POS}-{REF}-{ALT}-hg19",
    "Varsome": "https://varsome.com/variant/hg19/{CHROM}-{POS}-{REF}-{ALT}",
    "gnomAD_2": "https://gnomad.broadinstitute.org/variant/{CHROM}-{POS}-{REF}-{ALT}?dataset=gnomad_r2_1",
    "autopvs1": "https://autopvs1.bgi.com/variant/hg19/{CHROM}-{POS}-{REF}-{ALT}",
    "ClinVar": "https://www.ncbi.nlm.nih.gov/clinvar/?term={CHROM}-{POS}-{REF}-{ALT}"
  },
  "html_report_default_hidden_columns": [
    "QUAL",
    "AC",
    "FEATUREID",
    "AA_POS",
    "AA_LEN",
    "NMD_PERC",
    "dbNSFP_ALFA_Total_AC",
    "dbNSFP_REVEL_score",
    "splice_dbscSNV_rf_score",
    "splice_dbscSNV_ada_score",
    "splice_spidex_dpsi_zscore",
    "dbNSFP_clinvar_clnsig",
    "hgmd_CLASS",
    "proband_count",
    "control_count",
    "proband_variant_count",
    "control_variant_count",
    "proband_allele_count",
    "control_allele_count",
    "proband_homozygous_count",
    "control_homozygous_count"
  ],
  "html_report_truncate_settings": {
    "default_max_width_px": 120,
    "columns_for_hover_expand": [
      "VAR_ID",
      "HGVS_P",
      "HGVS_C",
      "ID",
      "REF",
      "ALT",
      "EFFECT",
      "IMPACT",
      "FEATUREID",
      "dbNSFP_clinvar_clnsig",
      "ClinVar_CLNSIG",
      "GT"
    ],
    "column_specific_max_widths_px": {
      "GT": 250
    }
  }
}
